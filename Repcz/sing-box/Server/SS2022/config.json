{
    "log": {
        "disabled": false,
        "level": "info",
        "timestamp": true
    },
    "dns": {
        "servers": [
            {"tag": "cloudflare","type": "https","server": "1.1.1.1"}
        ],
        "strategy": "ipv4_only",
        "rules": [
            {"query_type": ["A","AAAA"],"action": "route","server": "cloudflare","strategy": "ipv4_only"}
        ],
        "final": "cloudflare",
        "independent_cache": true
    },
    "inbounds": [
        {
            "type": "shadowsocks",
            "listen": "::",
            "listen_port": 65110,
            "method": "2022-blake3-aes-128-gcm",
            "password": "", // 执行 sing-box generate rand 16 --base64 生成
            "multiplex": {
                "enabled": true,
                "padding": true
            }
        }
    ],
    "outbounds": [
        {
            "type": "direct"
        }
    ],
    "ntp": {
        "enabled": true,
        "server": "time.apple.com",
        "server_port": 123,
        "interval": "30m"
    }
}